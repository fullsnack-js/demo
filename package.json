  "dependencies": {
    "@chakra-ui/react": "^1.6.7",
    "@chakra-ui/theme": "^1.10.1",
    "@chakra-ui/theme-tools": "^1.2.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/lexend": "^4.5.1",
    "@fontsource/oswald": "^4.5.1",
    "@fontsource/roboto-slab": "^4.5.0",
    "@fontsource/space-grotesk": "^4.5.0",
    "@fontsource/trispace": "^4.5.0",
    "@fontsource/libre-baskerville": "^4.5.0",
    "@fontsource/libre-franklin": "^4.5.0",
    "@fontsource/work-sans": "^4.5.1",
    "date-fns": "^2.23.0",
    "framer-motion": "^4",
    "next": "11.1.0",
    "next-seo": "^4.26.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "lint-staged": "^11.1.2",
    "next-sitemap": "^1.6.164",
    "prettier": "^2.3.2",
    "typescript": "4.3.5"
